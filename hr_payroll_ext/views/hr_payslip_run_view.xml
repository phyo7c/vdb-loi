<odoo>
    <!-- Add Payment button on the payslip run form. -->
    <record id="hr_payslip_run_view_form_inherit" model="ir.ui.view">
        <field name="name">hr.payslip.run.form.inherit.form</field>
        <field name="model">hr.payslip.run</field>
        <field name="inherit_id" ref="hr_payroll.hr_payslip_run_form"/>
        <field name="arch" type="xml">
            <xpath expr="//label[@for='date_start']" position="before">
                <field name="tax_approval_no" />
            </xpath>
        </field>
    </record>
</odoo>